{
  "name": "javascript-todo-list-tutorial",
  "description": "Learn how to build a Todo List in JavaScript following Test Driven Development TDD!",
  "version": "1.0.5",
  "homepage": "https://github.com/dwyl/todomvc-vanilla-javascript-elm-architecture-example",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwyl/todomvc-vanilla-javascript-elm-architecture-example.git"
  },
  "author": "@dwyl & friends!",
  "devDependencies": {
    "@types/node": "^22.5.3",
    "@types/tape": "^5.6.4",
    "decache": "^4.6.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "live-server": "^1.1.0",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "tap-nyc": "^1.0.3",
    "tape": "^5.0.1",
    "ts-loader": "^9.5.1",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "scripts": {
    "check-coverage": "nyc check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "retest": "PORT=8000 node_modules/.bin/nodemon -e ts --exec ts-node ./test/*.test.ts",
    "start": "node dist/server.js",
    "dev": "webpack serve --mode development --open",
    "test": "nyc ts-node node_modules/tape/bin/tape ./test/*.test.ts | tap-nyc",
    "build": "webpack --mode production && tsc lib/server.ts --outDir dist"
  },
  "keywords": [
    "Todo List",
    "TodoMVC",
    "functional",
    "fast",
    "learn",
    "beginner",
    "tutorial",
    "how to",
    "Elm",
    "architecture"
