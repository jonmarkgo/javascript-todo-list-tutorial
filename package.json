  "license": "ISC",
  "pre-commit": [
    "type-check",
    "lint",
    "test"
  ],
  "dependencies": {
    "@babel/parser": "^7.25.6",
    "@babel/types": "^7.25.6",
    "chai": "^5.1.1",
    "elmish": "^0.0.1",
    "expect": "^29.7.0",
