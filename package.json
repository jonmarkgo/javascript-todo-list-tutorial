    "@types/node": "^22.5.5",
    "@types/node-localstorage": "^1.3.3",
    "@types/qunit": "^2.19.10",
    "@types/tape": "^5.6.4",
    "decache": "^4.6.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
